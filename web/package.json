{
  "name": "@flightdigital/astro-starter",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && tsc --noEmit && astro build",
    "preview": "astro preview",
    "build:pre": "yarn build && yarn preview",
    "astro": "astro",
    "upgrade:latest": "yarn upgrade-interactive --latest",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "link:flightdeck": "yalc add @flight-digital/flightdeck && yarn install --force",
    "unlink:flightdeck": "yalc remove @flight-digital/flightdeck && yarn install --force",
    "update:flightdeck": "yalc update @flight-digital/flightdeck"
  },
  "dependencies": {
    "@astrojs/check": "0.4.1",
    "@astrojs/react": "3.0.9",
    "@flight-digital/flightdeck": "^1.0.24",
    "@linaria/core": "^6.0.0",
    "@linaria/react": "^6.0.0",
    "@sanity/astro": "1.3.0",
    "@sanity/image-url": "1.0.2",
    "@types/react": "18.2.24",
    "@types/react-dom": "18.2.8",
    "astro": "4.2.4",
    "astro-portabletext": "0.9.4",
    "astro-seo": "^0.8.0",
    "groq": "3.17.0",
    "locomotive-scroll": "^4.1.4",
    "prettier-plugin-astro": "0.12.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@portabletext/types": "2.0.7",
    "@sanity/types": "3.17.0",
    "@types/locomotive-scroll": "^4",
    "@wyw-in-js/vite": "^0.3.0",
    "eslint": "8.50.0",
    "prettier": "3.0.3",
    "typescript": "5.2.2",
    "vite": "^5.0.12"
  },
  "engines": {
    "node": ">= 16.12"
  },
  "packageManager": "yarn@4.0.2"
}
